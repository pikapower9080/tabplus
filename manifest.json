{
  "name": "Tab+",
  "description": "A customizable new tab replacement.",
  "version": "2.4",
  "manifest_version": 3,
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "host_permissions": [
    "https://www.bing.com/*"
  ],
  "permissions": ["storage"],
  "icons": {
    "16": "/icons/icon-16.png",
    "32": "/icons/icon-32.png",
    "48": "/icons/icon-48.png",
    "128": "/icons/icon-128.png"
  }
}
